<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean id="authoritiesDataService" class="mobileoffice.business.services.data.AuthoritiesDataServiceImpl">
    <constructor-arg ref="authoritiesRepository"/>
</bean>
<bean id="clientDataService" class="mobileoffice.business.services.data.ClientDataServiceImpl">
    <constructor-arg ref="clientRepository"/>
</bean>
<bean id="contractDataService" class="mobileoffice.business.services.data.ContractDataServiceImpl">
    <constructor-arg ref="contractRepository"/>
</bean>
<bean id="contractOptionRspDataService" class="mobileoffice.business.services.data.ContractOptionRspDataServiceImpl">
    <constructor-arg ref="contractOptionRspRepository"/>
</bean>
<bean id="managerDataService" class="mobileoffice.business.services.data.ManagerDataServiceImpl">
    <constructor-arg ref="managerRepository"/>
</bean>
<bean id="optionRelationsRspDataService" class="mobileoffice.business.services.data.OptionRelationsRspDataServiceImpl">
    <constructor-arg ref="optionRelationsRspRepository"/>
</bean>
<bean id="optionsDataService" class="mobileoffice.business.services.data.OptionsDataServiceImpl">
    <constructor-arg ref="optionsRepository"/>
</bean>
<bean id="tariffDataService" class="mobileoffice.business.services.data.TariffDataServiceImpl">
    <constructor-arg ref="tariffRepository"/>
</bean>
<bean id="tariffOptionsRspDataService" class="mobileoffice.business.services.data.TariffOptionsRspDataServiceImpl">
    <constructor-arg ref="tariffOptionsRspRepository"/>
</bean>
<bean id="usersDataService" class="mobileoffice.business.services.data.UsersDataServiceImpl">
    <constructor-arg ref="usersRepository"/>
</bean>
<bean id="vContractWithTariffDataService" class="mobileoffice.business.services.data.VContractWithTariffDataServiceImpl">
    <constructor-arg ref="vContractWithTariffRepository"/>
</bean>

</beans>