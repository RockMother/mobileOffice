<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean id="clientDataService" class="mobileoffice.business.services.data.ClientDataServiceImpl">
    <constructor-arg ref="clientRepository"/>
</bean>
<bean id="clientContractRspDataService" class="mobileoffice.business.services.data.ClientContractRspDataServiceImpl">
    <constructor-arg ref="clientContractRspRepository"/>
</bean>
<bean id="contractDataService" class="mobileoffice.business.services.data.ContractDataServiceImpl">
    <constructor-arg ref="contractRepository"/>
</bean>
<bean id="optionsDataService" class="mobileoffice.business.services.data.OptionsDataServiceImpl">
    <constructor-arg ref="optionsRepository"/>
</bean>
<bean id="tariffDataService" class="mobileoffice.business.services.data.TariffDataServiceImpl">
    <constructor-arg ref="tariffRepository"/>
</bean>
<bean id="tariffOptionsRspDataService" class="mobileoffice.business.services.data.TariffOptionsRspDataServiceImpl">
    <constructor-arg ref="tariffOptionsRspRepository"/>
</bean>

</beans>