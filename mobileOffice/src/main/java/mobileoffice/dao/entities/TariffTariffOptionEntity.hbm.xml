<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mobileoffice.dao.entities.TariffTariffOptionEntity" table="tariff_tariff_option"
           schema="mobileoffice">
        <id name="id" column="id"/>
        <property name="tariffId" column="tariff_id"/>
        <property name="tariffOptionId" column="tariff_option_id"/>
        <many-to-one name="tariffByTariffId" class="mobileoffice.dao.entities.TariffEntity">
            <column name="tariff_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="tariffOptionByTariffOptionId" class="mobileoffice.dao.entities.TariffOptionEntity">
            <column name="tariff_option_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>