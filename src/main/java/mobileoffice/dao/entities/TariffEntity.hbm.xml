<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mobileoffice.dao.entities.TariffEntity" table="tariff" schema="mobileoffice">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="price" column="price"/>
        <set name="contractsById" inverse="true">
            <key>
                <column name="tariff_id"/>
            </key>
            <one-to-many not-found="ignore" class="mobileoffice.dao.entities.ContractEntity"/>
        </set>
        <set name="tariffTariffOptionsById" inverse="true">
            <key>
                <column name="tariff_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="mobileoffice.dao.entities.TariffTariffOptionEntity"/>
        </set>
    </class>
</hibernate-mapping>