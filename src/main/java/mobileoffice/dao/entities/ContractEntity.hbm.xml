<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mobileoffice.dao.entities.ContractEntity" table="contract" schema="mobileoffice">
        <id name="id" column="id"/>
        <property name="number" column="number"/>
        <property name="tariffId" column="tariff_id"/>
        <many-to-one name="tariffByTariffId" class="mobileoffice.dao.entities.TariffEntity">
            <column name="tariff_id"/>
        </many-to-one>
        <set name="clientContractsById" inverse="true">
            <key>
                <column name="contract_id"/>
            </key>
            <one-to-many not-found="ignore" class="mobileoffice.dao.entities.ClientContractEntity"/>
        </set>
    </class>
</hibernate-mapping>